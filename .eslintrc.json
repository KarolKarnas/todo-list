{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-undef": "off",
		"class-methods-use-this": "off",
		"no-console": "off",
		"no-underscore-dangle": "off"
	}
}
